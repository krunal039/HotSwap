{
  "manifest_version": 3,
  "name": "HotSwap",
  "version": "1.0.0",
  "description": "Redirect URLs from production to localhost for development debugging (PCF, CRM, etc.)",
  "author": "Krunal Patel",
  "homepage_url": "https://github.com/krunal039/HotSwap",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
